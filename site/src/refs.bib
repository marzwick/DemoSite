@INPROCEEDINGS{Kumar2010-mp,
  title     = "The effect of soliciting demographic data on the performance of
               students on online tests",
  author    = "Kumar, Amruth N",
  booktitle = "2010 IEEE Frontiers in Education Conference (FIE)",
  publisher = "IEEE",
  pages     = "T1F--1--T1F--4",
  abstract  = "Stereotype threat was studied in the context of online testing in
               Computer Science. For the study, the student's identity was made
               salient by asking the student to identify sex and race, with no
               additional allusions to positive or negative stereotypes
               associated with any of their subgroups. It was found that the
               negative effect of entering demographic data before answering a
               test was more readily apparent on harder topics. The negative
               effect of stereotype threat on female and non-Caucasian students
               was significant among those who had not already mastered the
               material. On the harder topic in the study, after excluding those
               who had mastered the material, stereotype threat was found to
               negatively affect male and Caucasian students also, possibly
               because of “choking effect”. Given these results, it is
               recommended that demographic information should be solicited
               after rather than before online tests.",
  month     =  oct,
  year      =  2010
}

@ARTICLE{Stricker2015-ce,
  title     = "Stereotype threat, inquiring about test takers' race and gender,
               and performance on low-stakes tests in a large-scale assessment:
               Stereotype threat",
  author    = "Stricker, Lawrence J and Rock, Donald A and Bridgeman, Brent",
  journal   = "ETS Res. Rep. Ser.",
  publisher = "Wiley",
  volume    =  2015,
  number    =  1,
  pages     = "1--12",
  abstract  = "This study explores stereotype threat on low-stakes tests used in
               a large-scale assessment, math and reading tests in the Education
               Longitudinal Study of 2002 (ELS). Issues identified in laboratory
               research (though not observed in studies of high-stakes tests)
               were assessed: whether inquiring about their race and gender is
               related to the performance of Black and female test takers and,
               secondarily, whether this association is greater for test takers
               most identified with math and reading. After high school
               sophomores completed a questionnaire that included inquiries
               about their race and gender, only one change in test performance
               was consistent with expectations from stereotype-threat theory:
               Black test takers' math scores decreased. Their reading scores
               and young women's math scores did not decrease, and
               identification with math and reading did not moderate score
               decreases for Black test takers or women.",
  month     =  jun,
  year      =  2015,
  keywords  = "Stereotype threat; race; gender; Education Longitudinal Study;
               low-stakes tests",
  language  = "en"
}

