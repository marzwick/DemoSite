@article{shaffer_course-based_2014,
	title = {A {Course}-{Based} {Research} {Experience}: {How} {Benefits} {Change} with {Increased} {Investment} in {Instructional} {Time}},
	volume = {13},
	issn = {1931-7913},
	shorttitle = {A {Course}-{Based} {Research} {Experience}},
	url = {https://www.lifescied.org/doi/10.1187/cbe-13-08-0152},
	doi = {10.1187/cbe-13-08-0152},
	abstract = {There is widespread agreement that science, technology, engineering, and mathematics programs should provide undergraduates with research experience. Practical issues and limited resources, however, make this a challenge. We have developed a bioinformatics project that provides a course-based research experience for students at a diverse group of schools and offers the opportunity to tailor this experience to local curriculum and institution-specific student needs. We assessed both attitude and knowledge gains, looking for insights into how students respond given this wide range of curricular and institutional variables. While different approaches all appear to result in learning gains, we find that a significant investment of course time is required to enable students to show gains commensurate to a summer research experience. An alumni survey revealed that time spent on a research project is also a significant factor in the value former students assign to the experience one or more years later. We conclude: 1) implementation of a bioinformatics project within the biology curriculum provides a mechanism for successfully engaging large numbers of students in undergraduate research; 2) benefits to students are achievable at a wide variety of academic institutions; and 3) successful implementation of course-based research experiences requires significant investment of instructional time for students to gain full benefit.},
	language = {en},
	number = {1},
	urldate = {2022-05-10},
	journal = {CBE—Life Sciences Education},
	author = {Shaffer, Christopher D. and Alvarez, Consuelo J. and Bednarski, April E. and Dunbar, David and Goodman, Anya L. and Reinke, Catherine and Rosenwald, Anne G. and Wolyniak, Michael J. and Bailey, Cheryl and Barnard, Daron and Bazinet, Christopher and Beach, Dale L. and Bedard, James E. J. and Bhalla, Satish and Braverman, John and Burg, Martin and Chandrasekaran, Vidya and Chung, Hui-Min and Clase, Kari and DeJong, Randall J. and DiAngelo, Justin R. and Du, Chunguang and Eckdahl, Todd T. and Eisler, Heather and Emerson, Julia A. and Frary, Amy and Frohlich, Donald and Gosser, Yuying and Govind, Shubha and Haberman, Adam and Hark, Amy T. and Hauser, Charles and Hoogewerf, Arlene and Hoopes, Laura L. M. and Howell, Carina E. and Johnson, Diana and Jones, Christopher J. and Kadlec, Lisa and Kaehler, Marian and Silver Key, S. Catherine and Kleinschmit, Adam and Kokan, Nighat P. and Kopp, Olga and Kuleck, Gary and Leatherman, Judith and Lopilato, Jane and MacKinnon, Christy and Martinez-Cruzado, Juan Carlos and McNeil, Gerard and Mel, Stephanie and Mistry, Hemlata and Nagengast, Alexis and Overvoorde, Paul and Paetkau, Don W. and Parrish, Susan and Peterson, Celeste N. and Preuss, Mary and Reed, Laura K. and Revie, Dennis and Robic, Srebrenka and Roecklein-Canfield, Jennifer and Rubin, Michael R. and Saville, Kenneth and Schroeder, Stephanie and Sharif, Karim and Shaw, Mary and Skuse, Gary and Smith, Christopher D. and Smith, Mary A. and Smith, Sheryl T. and Spana, Eric and Spratt, Mary and Sreenivasan, Aparna and Stamm, Joyce and Szauter, Paul and Thompson, Jeffrey S. and Wawersik, Matthew and Youngblom, James and Zhou, Leming and Mardis, Elaine R. and Buhler, Jeremy and Leung, Wilson and Lopatto, David and Elgin, Sarah C. R.},
